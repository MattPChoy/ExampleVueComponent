  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationDir": "./dist",
    "rootDir": "./src",
    "moduleResolution": "node",
    "lib": ["ES2015", "DOM"],
    "types": ["vite/client", "vue"],
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "src/**/*.vue"],
}